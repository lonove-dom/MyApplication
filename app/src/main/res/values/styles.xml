<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/white</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentTransitions">true</item>
        <item name="android:windowAllowEnterTransitionOverlap">true</item>
        <item name="android:windowAllowReturnTransitionOverlap">true</item>
    </style>
    <style name="ActivityOptionsTheme" parent="Theme.AppCompat.Light">
    </style>
    <style name="Time_Window_anim" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/windowshowanim</item>
        <item name="android:windowExitAnimation">@anim/windowmissanim</item>
    </style>
    <style name="MyEditText" parent="Theme.AppCompat.Light" type="text/css"><item name="colorControlNormal">#d3d3d3</item>
        <item name="colorControlActivated">#d3d3d3</item></style>

</resources>
